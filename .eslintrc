{
    "rules": {
        "indent": 0,
        "no-await-in-loop": 0,
        "require-atomic-updates": 0
    },
    "globals": {
        "BigInt": true
    },
    "extends": ["nodemailer", "prettier"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "script"
    },
    "plugins": ["@babel"]
}
